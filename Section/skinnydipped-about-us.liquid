{{ 'swiper.css' | asset_url | stylesheet_tag }}
{{ 'swiper.js' | asset_url | script_tag }}


{% liquid
  assign laptop_padding_top = section.settings.laptop_top
  assign laptop_padding_bottom = section.settings.laptop_bottom
  assign mobail_padding_top = section.settings.mobail_top
  assign mobail_padding_bottom = section.settings.mobail_bottom
  assign bgcolor = section.settings.bgcolor
  assign border_color = section.settings.border_color
  assign border_width = section.settings.border_width
  assign rowdirection = section.settings.flexdirection
  assign mobail_rowdirection = section.settings.mobail_flexdirection
%}

<style>
  .id_{{ section.id}} h3, .id_{{ section.id}} p{
    margin:0;
  }
  .id_{{ section.id}}{
    background-color: {{bgcolor}};
    padding-top:{{laptop_padding_top}}px;
    padding-bottom:{{laptop_padding_bottom}}px;
  }

  .id_{{ section.id}} .container{
      width:100%;
      margin:0px auto;
      border-color:{{border_color}};
      border-width:{{border_width}};
      border-style:solid;
      box-sizing: border-box;
  }
    {% if section.settings.container == "container" %}
      .id_{{ section.id}} .container{
      max-width:1200px;    
    }
  {% elsif  section.settings.container == "container-full" %}
    .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px 2%;
    }
  {% endif %}
  
  .id_{{ section.id}} .container .row{
    margin:-30px;
    flex-direction: {{rowdirection}};
    align-items:{{section.settings.aling}} ;
    display: flex;
    flex-wrap: wrap;
  }
  .id_{{ section.id}} .container .row .col{
   width:50%;
  box-sizing: border-box;
  padding:30px
  }

   .id_{{ section.id}} .image_slider{
     position: relative;
     height: 100%;
   }
    .id_{{ section.id}} .swiper {
      width: 100%;
      height: 100%;
    }

     .id_{{ section.id}} .swiper-slide {
      text-align: center;
      font-size: 18px;
      background: #fff;
      display: flex;
      justify-content: center;
      align-items: center;
    }

      .id_{{ section.id}} .swiper-slide img {
      display: block;
      width: 100%;
      height: 100%;
    }



  @media(max-width:767px){
      .id_{{ section.id}}{
      padding-left:20px;
       padding-right:20px;
      padding-top:{{mobail_padding_top}}px;
      padding-bottom:{{mobail_padding_bottom}}px;
      text-align: {{section.settings.mobail_text_aling}};
      }

   .id_{{ section.id}} .btn_div {
     text-align: {{section.settings.mobail_text_aling}};
   }
     .id_{{ section.id}} .btn::after{ 
       text-align: {{section.settings.mobail_text_aling}};
     }
    
    .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px;
    }
    .id_{{ section.id}} .container .row{
    flex-direction: {{mobail_rowdirection}};
    margin:0px;
    row-gap:20px;
  }
  .id_{{ section.id}} .container .row .col{
     width:100%;
    padding:0px
  }

  }
</style>

<section class="id_{{ section.id }}">
  <div class="container">
            {% for block in section.blocks %}
          {% case block.type %}

      {% when 'title_section' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                }
                 }
              </style>

              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>
              {% endcase %}
        {% endfor %}    
    
    <div class="row flex wrap">
      <div class="col hello">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'icon' %}
              <style>
                .{{block.id}}.lago{
                  max-width:{{block.settings.iconwidth}}px;
                  margin-bottom:{{block.settings.iconmargin}}px;
                }
              </style>
              {% if block.settings.img_icon != blank %}
                <img
                  class="lago {{ block.id }}"
                  src="{{ block.settings.img_icon | image_url }}"
                  alt="{{ block.settings.img_icon.alt }}"
                  width="100"
                  height="100"
                >
              {% endif %}
            {% when 'title' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px;
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                }
                 }
              </style>

              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>

            {% when 'richtext' %}
              <style>
                 .{{block.id}}{
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   line-height:{{block.settings.line_height}}px;
                   color:{{block.settings.textcolor}};
                   margin-bottom:{{block.settings.margin_bottom}}px;
                   
                 }
                  .{{block.id}} ul{
                       padding-inline-start: 0;
                       margin-block: 0;
                       }
                
                  .{{block.id}} ul li{
                      list-style: none;
                      margin-bottom:10px;
                      display: flex;
                       }
                
                 .{{block.id}} ul li::before{
                   content: "\2713";
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   margin-right: 10px;
                   }
                
                  @media(max-width:767px){
                  .{{block.id}}{
                   font-size:{{block.settings.mobail_font_size}}px;
                   line-height:{{block.settings.mobail_line_height}}px;
                   margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                 }
                .{{block.id}} ul li::before{ font-size:{{block.settings.mobail_font_size}}px;}
                    
                  }
              </style>
              <div class="{{ block.id }}">{{ block.settings.text }}</div>

            {% when 'button' %}
               <style>
                .{{block.id}} .btn{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.font_size}}px;
                  color:{{block.settings.text_color}};
                  border-color:{{block.settings.border_color}};
                  border-width:{{block.settings.border_width}}px;
                  border-style:solid;
                  border-radius:{{block.settings.border_radius}}px;
                  background-color: {{block.settings.btn_color}};
                  margin-bottom:{{block.settings.margin_bottom}}px;
                  padding:{{block.settings.padding}};
                  text-align: center;
                  text-decoration: none;
                  display: inline-block;
                  transition-duration: .3s;
                  position: relative;
                }
                 .{{block.id}} .btn:hover{
                    background-color: {{block.settings.btn_color_hover}};
                    color:{{block.settings.text_color_hover}};
                    border-color:{{block.settings.border_color_hover}};
                 }
                
               {% if block.settings.btnafter != blank %}
                .{{block.id}} .btn::after{
                    content: "{{ block.settings.btnafter }}";
                    width: 100%;
                    position: absolute;
                    left: 0;
                    top: 120%;
                   text-align: left;
                   font-size: 14px;
                   line-height:18px;
                   font-weight: 400;
                  color:{{block.settings.text_bottom_color}};
                 }
                 {% endif %}
              

                 @media(max-width:767px){
                 .{{block.id}} .btn{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_font_size}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                  padding:{{block.settings.mobail_padding}};
                }
                   
                {%- if block.settings.btn_full_width -%} 
                     .{{block.id}} .btn{
                       width:100%;
                     }
                  
                 .{{ block.id }}{ text-align: center;}
                .{{block.id}} .btn::after{ text-align: center;}
                  
                {%- endif -%}
              

                 }
              </style>

              {% if block.settings.btnlink != blank %}
                <div class="{{ block.id }} btn_div">
                <a class="btn " href="{{ block.settings.btnlink }}">{{ block.settings.btntext }}</a>
                </div>
              {% endif %}


             {% when 'accordion' %}
               <style>
                .{{ block.id }} { margin-bottom:{{block.settings.margin_bottom}}px;} 
                .{{ block.id }} .pannel{ padding:15px 0px 8px;}

              .{{ block.id }} .faq .title {
                    display: flex;
                   align-items: center;
                    margin: 0;
                    padding: 15px 0;
                    border-style: solid;
                    border-width: 0px 0px 1px 0px;
                    border-color: {{block.settings.title_color}};
                    justify-content: space-between;
              }
              .{{ block.id }} .faq .title h4{
                    margin: 0;
                   width: calc(100% - 50px);
                   font-size:{{block.settings.title_font_size}}px;
                   font-weight: {{block.settings.title_font_weight}};
                   line-height: 120%;
                   color: {{block.settings.title_color}};
                cursor: pointer;
              }
               .{{ block.id }} .faq.active .title h4{
                       color: {{block.settings.active_color}};
                   }
              .{{ block.id }} .faq .title.active svg{transform: rotate(0deg);}
                 
              .{{ block.id }} .faq .title svg{
                 transform: rotate(90deg);
                transition-duration: .3s;
                width:{{block.settings.icon_width}}px;
                color: {{block.settings.icon_color}};
              }
              .{{ block.id }} .faq p {
                    font-size:{{block.settings.text_font_size}}px;
                   font-weight: 400;
                   line-height: 130%;
                   color: {{block.settings.text_color}};
              }

              @media(max-width:767px){
               .{{ block.id }} { margin-bottom:{{block.settings.mobail_margin_bottom}}px; } 
               .{{ block.id }} .pannel{padding:10px 0px 8px;}
              .{{ block.id }} .faq .title h4{ font-size:{{block.settings.mobail_title_font_size}}px;}
              .{{ block.id }} .faq .title svg{ width:{{block.settings.mobail_icon_width}}px;}
              .{{ block.id }} .faq p { font-size:{{block.settings.mobail_text_font_size}}px; }
              }
           </style>

            
        <div class="accordion_main {{ block.id }}">
             {% for block in section.blocks %}
              {% case block.type %}
              {% when 'accordion_list' %}
     
        <div class="faq">
            <div class="accordion {{ block.id }} title">
              <h4>{{ block.settings.title }}</h4>
                   <svg viewBox="0 0 17 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                         <path d="M5.64645 2.64645C5.84171 2.45118 6.15829 2.45118 6.35355 2.64645L11.3536 7.64645C11.5488 7.84171 11.5488 8.15829 11.3536 8.35355L6.35355 13.3536C6.15829 13.5488 5.84171 13.5488 5.64645 13.3536C5.45118 13.1583 5.45118 12.8417 5.64645 12.6464L10.2929 8L5.64645 3.35355C5.45118 3.15829 5.45118 2.84171 5.64645 2.64645Z" fill="currentColor"></path>
                  </svg>
            </div>
            <div class="pannel"  style="display: none;">
               <p> {{ block.settings.text }}</p>
           </div>
      </div>
           {% endcase %}
        {% endfor %}           
     </div>
            
          {% endcase %}
        {% endfor %}
      </div>
      <div class="col">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'image' %}
            <style>
              .{{ block.id }} img{
                 aspect-ratio: {{block.settings.aspect_ratio}};
                 object-fit: {{block.settings.object_fit}};
                 border-radius:{{block.settings.border_radius}}{{block.settings.border_px}};
                background-color:{{block.settings.bg}} ;
              }
              .image_top_text{
                  font-size: 24px;
                  line-height: 130%;
                 font-weight: 600;
                  color:{{block.settings.text_color}};
                 text-align: center;
                margin:0px 0px 30px !important;
              }

              @media(max-width:767px){
                 .image_top_text{
                    font-size: 18px;
                     margin:0px 0px 20px !important;
                    } 
                   
              }
              
            </style>
             {% if block.settings.text != blank %}
            <h3 class="image_top_text">{{ block.settings.text }}</h3>
               {% endif %}
          <div class="image_slider {{ block.id }}">
              <div class="swiper imageSwiper{{ section.id }}">
                 <div class="swiper-wrapper">
                   {% if block.settings.img1 != blank %}
                        <div class="swiper-slide">
                     <img
                       src="{{ block.settings.img1 | image_url }}"
                       alt="{{ block.settings.img1.alt }}"
                       width="100"
                       height="100"
                       >
                        </div>
                    {% endif %}
                    {% if block.settings.img2 != blank %}
                        <div class="swiper-slide">
                     <img
                       src="{{ block.settings.img2 | image_url }}"
                       alt="{{ block.settings.img2.alt }}"
                       width="100"
                       height="100"
                       >
                        </div>
                    {% endif %}
                    {% if block.settings.img3 != blank %}
                        <div class="swiper-slide">
                     <img
                       src="{{ block.settings.img3 | image_url }}"
                       alt="{{ block.settings.img3.alt }}"
                       width="100"
                       height="100"
                       >
                        </div>
                    {% endif %}
                    {% if block.settings.img4 != blank %}
                        <div class="swiper-slide">
                     <img
                       src="{{ block.settings.img4 | image_url }}"
                       alt="{{ block.settings.img4.alt }}"
                       width="100"
                       height="100"
                       >
                        </div>
                    {% endif %}
                    {% if block.settings.img5 != blank %}
                        <div class="swiper-slide">
                     <img
                       src="{{ block.settings.img5 | image_url }}"
                       alt="{{ block.settings.img5.alt }}"
                       width="100"
                       height="100"
                       >
                        </div>
                    {% endif %}
                </div>
             </div>
          </div>

          {% endcase %}
        {% endfor %}
      </div>
    </div>
  </div>
</section>

      {% for block in section.blocks %}
          {% case block.type %}
              {% when 'accordion_list' %}
  <script>
 var acc = document.getElementsByClassName("{{ block.id }}");
      var i;

      for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {
          this.classList.toggle("active");
          this.parentElement.classList.toggle("active");

          var pannel = this.nextElementSibling;

          if (pannel.style.display === "block") {
            pannel.style.display = "none";
          } else {
            pannel.style.display = "block";
          }
        });
      }
</script>

            {% when 'image' %}

  <script>
    
    var swiper = new Swiper(".imageSwiper{{ section.id }}", {
      spaceBetween: 30,
      centeredSlides: true,
      autoplay: {
        delay: 2500,
        disableOnInteraction: false,
      },
      pagination: {
        el: ".swiper-pagination",
        clickable: true,
      },
      navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
    
  
</script>
      {% endcase %}
        {% endfor %}
            

{% schema %}
