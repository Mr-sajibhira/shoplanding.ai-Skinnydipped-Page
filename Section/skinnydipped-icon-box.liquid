

{% liquid
  assign laptop_padding_top = section.settings.laptop_top
  assign laptop_padding_bottom = section.settings.laptop_bottom
  assign mobail_padding_top = section.settings.mobail_top
  assign mobail_padding_bottom = section.settings.mobail_bottom
  assign bgcolor = section.settings.bgcolor
%}

<style>
  .id_{{ section.id}}{
    background-color: {{bgcolor}};
    padding-top:{{laptop_padding_top}}px;
    padding-bottom:{{laptop_padding_bottom}}px;
    background-image:url({{section.settings.bgimg | image_url}});
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
  }

 .id_{{ section.id}} .container{
   width:100%;
   margin:0px auto;
   box-sizing: border-box;
 }
  
  {% if section.settings.container == "container" %}
      .id_{{ section.id}} .container{
      max-width:1200px;    
    }
  {% elsif  section.settings.container == "container-full" %}
    .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px 2%;
    }
  {% endif %}

.id_{{ section.id}} .row{
    display: flex;
    flex-wrap: wrap;
  margin:-{{section.settings.box_gap}}px;
}
 .id_{{ section.id}} .row .col{
    max-width: calc(100% / {{section.settings.box_show}});
   width:100%;
   padding:{{section.settings.box_gap}}px;
   box-sizing: border-box;
 } 

  .id_{{ section.id}} .row .col .icon_box{
   padding:{{section.settings.box_padding}}px;
   background-color: {{section.settings.box_color}};
    height:100%;
  }


  
    .id_{{ section.id}} .row .col .icon_box img{
      display: block;
       width:100%;
      height:{{section.settings.icon_height}};
       max-width:{{section.settings.icon_width}};
      aspect-ratio:{{section.settings.aspect_ratio}};
      object-fit:{{section.settings.object_fit}};
      margin-bottom: {{ section.settings.icon_margin }}px;
    }

   .id_{{ section.id}} .row .col .icon_box h3{
        font-size:{{section.settings.title_font_size}}px;
        line-height:{{section.settings.title_line_height}}px;
        font-weight:{{section.settings.title_font_weight}};
        margin: 0;
        margin-bottom:{{section.settings.title_margin_bottom}}px;
      color:{{section.settings.title_color}};
   }
     .id_{{ section.id}} .row .col .icon_box p{
        font-size:{{section.settings.text_font_size}}px;
        line-height:{{section.settings.text_line_height}}px;
        font-weight:{{section.settings.text_font_weight}};
        margin: 0;
        margin-bottom:{{section.settings.text_margin_bottom}}px;
       color:{{section.settings.text_color}};
   }


  @media(max-width:767px){
      .id_{{ section.id}}{
       padding-left:20px;
       padding-right:20px;
      padding-top:{{mobail_padding_top}}px;
      padding-bottom:{{mobail_padding_bottom}}px;
      }
      .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px;
    }
    .id_{{ section.id}} .row{
     margin:-{{section.settings.mobail_box_gap}}px;
}
 .id_{{ section.id}} .row .col{
   max-width:100%;
   padding:{{section.settings.mobail_box_gap}}px;
 } 

  .id_{{ section.id}} .row .col .icon_box{
   padding:{{section.settings.mobail_box_padding}}px;
  }

    .id_{{ section.id}} .row .col .icon_box img{
      height:{{section.settings.mobail_icon_height}};
       max-width:{{section.settings.mobaile_icon_width}};
      margin-bottom: {{ section.settings.mobail_icon_margin }}px;
    }

   .id_{{ section.id}} .row .col .icon_box h3{
        font-size:{{section.settings.mobaile_title_font_size}}px;
        line-height:{{section.settings.mobaile_title_line_height}}px;
        margin-bottom:{{section.settings.mobaile_title_margin_bottom}}px;
   }
     .id_{{ section.id}} .row .col .icon_box p{
        font-size:{{section.settings.mobaile_text_font_size}}px;
        line-height:{{section.settings.mobail_text_line_height}}px;
        margin-bottom:{{section.settings.mobail_text_margin_bottom}}px;
   }



  }
</style>

<section class="id_{{ section.id }}">
  <div class="container">
        {% for block in section.blocks %}
          {% case block.type %}
                {% when 'title' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                }
                 }
              </style>
              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>
              {% when 'richtext' %}
              <style>
                 .{{block.id}}{
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   line-height:{{block.settings.line_height}}px;
                   color:{{block.settings.textcolor}};
                   margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                 }
                    .{{block.id}} ul{
                             padding-inline-start: 0;
                       margin-block: 0;
                       }
                       .{{block.id}} ul li{
                          list-style: none;
                         margin-bottom:10px;
                          display: flex;
                       }
                 .{{block.id}} ul li::before{

                   content: "\2713";
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   margin-right: 10px;
                   }
                  @media(max-width:767px){
                  .{{block.id}}{
                   font-size:{{block.settings.mobail_font_size}}px;
                   line-height:{{block.settings.mobail_line_height}}px;
                   margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                 }
                .{{block.id}} ul li::before{
                      font-size:{{block.settings.mobail_font_size}}px;
                     }
                  }
              </style>
              <div class="{{ block.id }}">{{ block.settings.text }}</div>
             
          {% endcase %}
      {% endfor %} 

    <div class="row">
              {% for block in section.blocks %}
          {% case block.type %}
                {% when 'iconbox' %}
            <style>
          .{{ block.id }} .icon_box{
     text-align: {{block.settings.text_align}};
  }

    {% if block.settings.text_align == "left" %}
    .{{ block.id }} .icon_box img{
   margin:0px auto 0px 0px;
   }
  {% elsif  block.settings.text_align == "center" %}
      .{{ block.id }} .icon_box img{
   margin:0px auto;
   }
  {% elsif  block.settings.text_align == "right" %}
      .{{ block.id }} .icon_box img{
   margin:0px 0px 0px auto;
   }
  {% endif %}

            @media(max-width:767px){

          .{{ block.id }} .icon_box{
     text-align: {{block.settings.mtext_align}};
  }

    {% if block.settings.mtext_align == "left" %}
    .{{ block.id }} .icon_box img{
   margin:0px auto 0px 0px;
   }
  {% elsif  block.settings.mtext_align == "center" %}
      .{{ block.id }} .icon_box img{
   margin:0px auto;
   }
  {% elsif  block.settings.mtext_align == "right" %}
      .{{ block.id }} .icon_box img{
   margin:0px 0px 0px auto;
   }
  {% endif %}


              
            }

              
              
            </style>
                    <div class="col {{ block.id }}">
                        <div class="icon_box">
                    {% if block.settings.icon_id != blank %}
                         <div class="icon_img">
                <img class=""
                  src="{{ block.settings.icon_id | image_url }}"
                  alt="{{ block.settings.icon_id.alt }}"
                  width="100%"
                  height="100%"
                >
                       </div>
                {% endif %}
                           {% if block.settings.title != blank %}
                               <h3>{{ block.settings.title }}</h3>
                          {% endif %}
                          {% if block.settings.text != blank %}
                              <p>{{ block.settings.text }}</p>
                          {% endif %}
                    </div>
             </div>

                {% endcase %}
      {% endfor %} 
    </div>

    
    
  </div>
</section>




