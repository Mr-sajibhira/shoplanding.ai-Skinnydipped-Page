         "type": "text",
         "id": "img_width",
        "label": "Image Width",
        "default":"115px"
      },
      {
      "type": "text",
      "id": "aspect_ratio",
      "label": "Image Aspect Ratio(1/1)",
       "default":"1/1"
    },
     {
        "type": "select",
         "id": "object_fit",
            "label": "Image Object fit",
            "options": [
              {
                "label": "fill",
                "value": "fill"
              },
              {
                "label": "cover",
                "value": "cover"
              },
              {
                "value": "contain",
                "label": "contain"
              }
