

{% liquid
  assign laptop_padding_top = section.settings.laptop_top
  assign laptop_padding_bottom = section.settings.laptop_bottom
  assign mobail_padding_top = section.settings.mobail_top
  assign mobail_padding_bottom = section.settings.mobail_bottom
  assign bgcolor = section.settings.bgcolor
  assign border_color = section.settings.border_color
  assign border_width = section.settings.border_width
%}

<style>
  img {
    display: block;
    max-width: 100%;
    width: 100%;
    height: auto;
}
  .id_{{ section.id}}{
    background-color: {{bgcolor}};
    padding-top:{{laptop_padding_top}}px;
    padding-bottom:{{laptop_padding_bottom}}px;
  }

  .id_{{ section.id}} .container{
      border-color:{{border_color}};
      border-width:{{border_width}};
      border-style: solid;
      box-sizing: border-box;
      width:100%;
      margin:0px auto;
  }

    {% if section.settings.container == "container" %}
      .id_{{ section.id}} .container{
      max-width:1200px;    
    }
  {% elsif  section.settings.container == "container-full" %}
    .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px 5%;
    }
  {% endif %}    


  @media(max-width:767px){
      .id_{{ section.id}}{
       padding-left:20px;
       padding-right:20px;
       padding-top:{{mobail_padding_top}}px;
       padding-bottom:{{mobail_padding_bottom}}px;
      }

    .id_{{ section.id}} .container{
      max-width:100%;
      padding:0px;
    }


  }
</style>

<section class="id_{{ section.id }}">
  <div class="container">
        {% for block in section.blocks %}

          {% case block.type %}
                {% when 'title' %}
              <style>
                .{{block.id}}.title{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.line_height}}px;
                  color:{{block.settings.textcolor}};
                  margin-bottom:{{block.settings.margin_bottom}}px !important;
                  text-align: {{block.settings.text_align}};
                  width:{{block.settings.width}}%;
                  margin:{{block.settings.align_content}}
                }
                 @media(max-width:767px){
                 .{{block.id}}.title{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_line_height}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                  text-align: {{block.settings.mobail_text_align}};
                  width:{{block.settings.mobail_width}}%;
                  margin:{{block.settings.mobail_align_content}}
                }
                 }
              </style>

              <h3 class="title {{ block.id }}">{{ block.settings.title }}</h3>
              {% when 'richtext' %}
              <style>
                 .{{block.id}}{
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   line-height:{{block.settings.line_height}}px;
                   color:{{block.settings.textcolor}};
                   margin-bottom:{{block.settings.margin_bottom}}px !important;
                   text-align: {{block.settings.text_align}};
                   width:{{block.settings.width}}%;
                   margin:{{block.settings.align_content}}
                   }
                  .{{block.id}} ul{
                      padding-inline-start: 0;
                      margin-block: 0;
                  }
                  .{{block.id}} ul li{
                      list-style: none;
                      margin-bottom:10px;
                     display: flex;
                       }
                 .{{block.id}} ul li::before{
                   content: "\2713";
                   font-size:{{block.settings.font_size}}px;
                   font-weight:{{block.settings.font_weight}};
                   margin-right: 10px;
                   }
                
                  @media(max-width:767px){
                  .{{block.id}}{
                   font-size:{{block.settings.mobail_font_size}}px;
                   line-height:{{block.settings.mobail_line_height}}px;
                   margin-bottom:{{block.settings.mobail_margin_bottom}}px !important;
                   text-align: {{block.settings.mobail_text_align}};
                   width:{{block.settings.mobail_width}}%;
                   margin:{{block.settings.mobail_align_content}}
                 }
                 .{{block.id}} ul li::before{
                      font-size:{{block.settings.mobail_font_size}}px;
                  }
                    
                  }
              </style>
              <div class="{{ block.id }}">{{ block.settings.text }}</div>
                       {% when 'button' %}
              <style>
                .{{block.id}} .btn{
                  font-size:{{block.settings.font_size}}px;
                  font-weight:{{block.settings.font_weight}};
                  line-height:{{block.settings.font_size}}px;
                  color:{{block.settings.text_color}};
                  border-color:{{block.settings.border_color}};
                  border-width:{{block.settings.border_width}}px;
                  border-style:solid;
                  border-radius:{{block.settings.border_radius}}px;
                  background-color: {{block.settings.btn_color}};
                  margin-bottom:{{block.settings.margin_bottom}}px;
                  padding:{{block.settings.padding}};
                  transition-duration: .5s;
                  text-align: center;
                  text-decoration: none;
                  display: inline-block;
                }
                 .{{block.id}} .btn:hover{
                    background-color: {{block.settings.btn_color_hover}};
                    color:{{block.settings.text_color_hover}};
                    border-color:{{block.settings.border_color_hover}};
                 }
                 .{{block.id}}{
                    text-align: {{block.settings.text_align}};
                 }
                 @media(max-width:767px){
                 .{{block.id}} .btn{
                  font-size:{{block.settings.mobail_font_size}}px;
                  line-height:{{block.settings.mobail_font_size}}px;
                  margin-bottom:{{block.settings.mobail_margin_bottom}}px;
                  padding:{{block.settings.mobail_padding}};
                }
                .{{block.id}}{
                    text-align: {{block.settings.mobail_text_align}};
                 }
                 }
              </style>

              {% if block.settings.btnlink != blank %}
                <div class="{{ block.id }}">
                <a class="btn" href="{{ block.settings.btnlink }}">{{ block.settings.btntext }}</a>
                </div>
              {% endif %}

            
             {% when 'customer_box' %}
                <style>
              .{{ block.id }}{
                
              }
                  
            .{{ block.id }} .row{
                  display: flex;
                 flex-wrap: wrap;
                 margin:calc(-{{block.settings.box_gap}}px / 2);
              }
                  
            .{{ block.id }} .row .col{
              max-width:33.33%;
              width:100%;
              padding:calc({{block.settings.box_gap}}px / 2);
              box-sizing: border-box;
              height: auto;
            }
                  
            .{{ block.id }} .row .col .customer_box{
               padding:{{block.settings.box_padding}}px;
               box-sizing: border-box;
               background-color: {{block.settings.box_bg}};
              border-radius:5px;
              height: 100%;
             }
                  
             .{{ block.id }} .row .col .customer_box .content{
                display: flex;
                flex-wrap: wrap;
                gap:20px;
               margin:0px 0px {{block.settings.box_margin}}px 0px;
             }
                  
            .{{ block.id }} .row .col .customer_box .content img{
                max-width:{{block.settings.img_width}};
                aspect-ratio:{{block.setting.aspect_ratio}};
                object-fit:{{block.setting.object_fit}};
                margin:0px auto;
               border-radius:100%;
              }     
              .{{ block.id }} .row .col .customer_box .content .info{
                 display: flex;
                 flex-direction: column;
                justify-content: space-between;
              }
              .{{ block.id }} .row .col .customer_box .content .info h3{
                font-size:{{block.settings.name_font}}px;
                font-weight:700;
                line-height:150%;
                color:{{block.settings.name_color}};
                margin:0px
              } 
              .{{ block.id }} .row .col .customer_box .content .info h4{
                font-size:{{block.settings.user_font}}px;
                font-weight:700;
                line-height:150%;
                color:{{block.settings.username_color}};
                margin:0px;
              }  
              .{{ block.id }} .row .col .customer_box .content .info p{
                padding:7px 15px;
                margin:0px;
                display: inline-block;
                font-size:13px;
                font-weight:400;
                line-height:100%;
                color: #ffffff;
                background-color: {{block.settings.varified_color}};
                border-radius:30px;
                cursor: pointer;
              }
              .{{ block.id }} .row .col .customer_box .text{
                font-size:{{block.settings.text_font}}px;
                font-weight:400;
                line-height:150%;
                color:{{block.settings.text_color}};
                margin:0px;
              }
                  
               @media(max-width:767px){     
                 
            .{{ block.id }} .row{ margin:calc(-{{block.settings.mobail_box_gap}}px / 2);}
                  
            .{{ block.id }} .row .col{
              max-width:100%;
              padding:calc({{block.settings.mobail_box_gap}}px / 2);
            }
                  
            .{{ block.id }} .row .col .customer_box{ padding:{{block.settings.mobail_box_padding}}px;}  
            .{{ block.id }} .row .col .customer_box .content{ margin:0px 0px {{block.settings.mobail_box_margin}}px 0px;}                  
            .{{ block.id }} .row .col .customer_box .content img{ max-width:{{block.settings.mobail_img_width}};}     
            .{{ block.id }} .row .col .customer_box .content .info h3{ font-size:{{block.settings.mobail_name_font}}px;} 
            .{{ block.id }} .row .col .customer_box .content .info h4{font-size:{{block.settings.mobail_user_font}}px;}  
            .{{ block.id }} .row .col .customer_box .text{font-size:{{block.settings.mobail_text_font}}px;}                 
                  
               } 
             </style>
            
               <div class="customer_main {{ block.id }}">
                 <div class="row">
                   {% for block in section.blocks %}
                    {% case block.type %}
                      {% when 'customer_box_list' %}
                          <div class="col">
                             <div class="customer_box">
                                <div class="content">
                                    <div class="div_img">
                                         {% if block.settings.img != blank %}
                                            <img
                                                 src="{{ block.settings.img | image_url }}"
                                                 alt="{{ block.settings.img.alt }}"
                                                 width="100%"
                                                 height="100%"
                                              >
                                          {% endif %}
                                    </div>

                                     <div class="info">
                                          <h3>{{ block.settings.name }}</h3>
                                          <h4>{{ block.settings.username }}</h4>
                                          <p>Verified Purchase</p>
                                       
                                     </div>
                                </div>

                               <p class="text">{{ block.settings.text }}</p>
                             </div>
                          </div>
                       {% endcase %}
                     {% endfor %} 
                   </div>
              </div>

          {% endcase %}
      {% endfor %} 
    
  </div>
</section>


